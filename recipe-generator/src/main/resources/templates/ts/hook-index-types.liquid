{% for ingredient in ingredients %}
{%- assign hookName = 'Abstract' | append:ingredient.name | append:'Hook' -%}
export { {{hookName}} } from "./{{hookName}}";
{% endfor %}
{% for ingredient in ingredients %}
{%- assign dataSnapshotName = ingredient.name | append:'Data' -%}
export { {{dataSnapshotName}} } from "./{{dataSnapshotName}}";
{% endfor %}
{% for enum in enums %}
export { {{enum.name}} } from "./{{enum.name}}";
{% endfor %}